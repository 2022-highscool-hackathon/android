<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="start_btn_background">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_marginStart">55dp</item>
        <item name="android:layout_marginEnd">55dp</item>
        <item name="android:layout_marginTop">30dp</item>
        <item name="android:elevation">10dp</item>
    </style>
    <style name="start_btn_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:fontFamily">@font/pretendard_semibold</item>
        <item name="android:includeFontPadding">false</item>
    </style>
    <style name="start_btn_iamge">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginEnd">22dp</item>
    </style>
</resources>